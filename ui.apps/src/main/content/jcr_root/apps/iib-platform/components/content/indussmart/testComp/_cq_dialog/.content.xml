<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Help Me Component Dialog"
    jcr:primaryType="nt:unstructured"
    jcr:title="Help Me Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <config
                jcr:primaryType="nt:unstructured"
                jcr:title="Configurations"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="User Name Label"
                                name="./labelName"/>
                            <phoneNo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="User Phone No Label"
                                name="./labelPhoneNo"/>
                            <emailId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="User Email Label"
                                name="./labelEmailId"/>
                            <buttonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Button Text"
                                name="./buttonText"/>
                            <checkBoxText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Check Box Text"
                                name="./checkBoxText"/>
                            <namePlaceHolder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="User Name Place Holder"
                                name="./namePlaceHolder"/>
                            <phonePlaceHolder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Phone No Place Holder"
                                name="./phonePlaceHolder"/>
                            <emailPlaceHolder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Email Place Holder"
                                name="./emailPlaceHolder"/>
                        </items>
                    </column>
                </items>
            </config>
            <view
                jcr:primaryType="nt:unstructured"
                jcr:title="View"
                sling:resourceType="granite/ui/components/foundation/include"
                path="iib-platform/components/content/common/dialogTabs/view"/>
        </items>
    </content>
</jcr:root>
