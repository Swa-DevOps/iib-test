<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Create Enach Form"
    jcr:primaryType="nt:unstructured"
    jcr:title="Create Enach Form"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <config
                jcr:primaryType="nt:unstructured"
                jcr:title="Configurations"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <backHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Back Heading"
                                name="./backHeading"/>
                            <backUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Back Button Url"
                                name="./backButtonUrl"
                                rootPath="/content/enach"/>
                            <eNachHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="E-NACH Heading"
                                name="./eNachHeading"/>
                            <debitDetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Debit Details Heading"
                                name="./debitDetails"/>
                            <imagePath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="eNachOffers"
                                name="./imagePath"
                                rootPath="/content/dam"/>
                            <accNameHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Until Cancel Text"
                                name="./untilCancel"/>
                            <bankNameHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Account Name Heading"
                                name="./accNameHeading"/>
                            <accountNumberHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Account Number"
                                name="./accountNumber"/>
                            <confirmAccountNumberHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Confirm Account Number"
                                name="./confirmAccountNumberHeading"/>
                            <bankIFSCcodeHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="BANK IFSC Code"
                                name="./bankIFSCcodeHeading"/>
                            <emiamountToTransferHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Amount To Transfer"
                                name="./emiamountToTransferHeading"/>
                            <amountToTransferHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Amount To Transfer"
                                name="./amountToTransferHeading"/>
                            <transferFrequencyHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Transfer Frequency"
                                name="./transferFrequencyHeading"/>
                            <startDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Start Date"
                                name="./startDate"/>
                            <untilCancel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Until Cancel Text"
                                name="./untilCancel"/>
                            <endDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="End Date"
                                name="./endDate"/>
                            <monthly
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Monthly Frequency"
                                name="./monthly"/>
                            <weekly
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Weekly Frequency"
                                name="./weekly"/>
                            <transferFrequency
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Transfer Frequency">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested=""
                                    name="./transferFrequency">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <text
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Text"
                                                    name="./text"/>
                                                <value
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Value"
                                                    name="./value"
                                                    rootPath="/content/indusind"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </transferFrequency>
                        </items>
                    </column>
                </items>
            </config>
            <creditConfig
                jcr:primaryType="nt:unstructured"
                jcr:title="Credit Configurations"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <totalAmount
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Total Amount"
                                name="./totalAmount"/>
                            <totalInstallments
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Total Installments"
                                name="./totalInstallments"/>
                            <aadhaarVirtualID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel=" Aadhaar/Virtual ID"
                                name="./aadhaarVirtualId"/>
                            <generateVID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Generate VID"
                                name="./generateVID"/>
                            <referralCode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Referral Code"
                                name="./referralCode"/>
                            <creditDetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Credit Details Heading"
                                name="./creditDetails"/>
                            <creditAccountHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Credit Account Heading"
                                name="./creditAccountHeading"/>
                        </items>
                    </column>
                </items>
            </creditConfig>
            <dynamicData
                jcr:primaryType="nt:unstructured"
                jcr:title="Dynamic Data"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <rdStatement
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="RD Statement"
                                name="./rdStatement"/>
                            <rdAmount
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="RD amount"
                                name="./rdAmount"/>
                            <maturityDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Maturity Date"
                                name="./maturityDate"/>
                            <rateInterest
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Rate Of Interest"
                                name="./rateInterest"/>
                            <maturityAmount
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Maturity Amount"
                                name="./maturityAmount"/>
                            <nomineeStatement
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Nominee Statement"
                                name="./nomineeStatement"/>
                            <comment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Comment"
                                name="./comment"/>
                            <confirm
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Confirm"
                                name="./confirm"/>
                            <cancel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Cancel"
                                name="./cancel"/>
                            <cancelLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBlank="true"
                                autoDelete="{Boolean}true"
                                fieldLabel="Cancel Button URL"
                                name="./cancelButtonUrl"
                                rootPath="/content/enach"/>
                        </items>
                    </column>
                </items>
            </dynamicData>
            <view
                jcr:primaryType="nt:unstructured"
                jcr:title="View"
                sling:resourceType="granite/ui/components/foundation/include"
                path="iib-platform/components/content/common/dialogTabs/view"/>
        </items>
    </content>
</jcr:root>
