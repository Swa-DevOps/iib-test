<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="E-Nach Details"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
            <config
                jcr:primaryType="nt:unstructured"
                jcr:title="Configurations"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <backButtonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Back Button Text"
                                name="./backButtonText"/>
                            <backButtonUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide URL for Back Button Hyperlink"
                                fieldLabel="Back Button Hyperlink URL"
                                name="./backButtonUrl"
                                rootPath="/content/enach"/>
                            <sectionHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Section Heading"
                                name="./sectionHeading"/>
                            <buttonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Button Text"
                                name="./buttonText"/>
                            <buttonUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide URL for Button Hyperlink"
                                fieldLabel="Button Hyperlink URL"
                                name="./buttonUrl"
                                rootPath="/content/indusindenach"/>
                        </items>
                    </columns>
                </items>
            </config>
            <view
                jcr:primaryType="nt:unstructured"
                jcr:title="View"
                sling:resourceType="granite/ui/components/foundation/include"
                path="iib-platform/components/content/common/dialogTabs/view"/>
        </items>
    </content>
</jcr:root>
